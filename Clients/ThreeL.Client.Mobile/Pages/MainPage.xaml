<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="ThreeL.Client.Mobile.Pages.MainPage"
            SelectedTabColor="{StaticResource PrimaryColor}"
            UnselectedTabColor="#aaa"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom"
            xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
            xmlns:pages ="clr-namespace:ThreeL.Client.Mobile.Pages.Content">
    <TabbedPage.Behaviors>
        <toolkit:EventToCommandBehavior EventName="Loaded" Command="{Binding LoadedCommandAsync}" />
    </TabbedPage.Behaviors>
</TabbedPage>